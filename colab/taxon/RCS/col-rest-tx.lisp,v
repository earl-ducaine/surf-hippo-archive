#+Genera 
(defun load-taxon ()
  (let ((tv:more-processing-global-enable nil)	 ; suppress **MORE**
	(sys:inhibit-fdefine-warnings :just-warn)); redefine but
                                                        	   ; show warnings
    (pdefsys:load-system 'TAXON :source-if-newer t)
    ))
