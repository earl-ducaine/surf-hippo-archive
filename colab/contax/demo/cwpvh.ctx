;;;  define domain of all words:

(cn:def-domain word 
    (word2 word3 word4 word5 word6 word7 word8 word9 word10))

;;;  define singleton domains for constants 1 to 10:

(cn:def-domain position1 (1))
(cn:def-domain position2 (2))
(cn:def-domain position3 (3))
(cn:def-domain position4 (4))
(cn:def-domain position5 (5))
(cn:def-domain position6 (6))
(cn:def-domain position7 (7))
(cn:def-domain position8 (8))
(cn:def-domain position9 (9))
(cn:def-domain position10 (10))
(cn:def-domain position 
    (position1 position2 position3 position4 position5 
     position6 position7 position8 position9 position10))

;;;  define the basic constraint sameletter:

(cn:def-lisp-constraint sameletter 
   :interface (w1 p1 w2 p2) 
   :domains (word position word position)
   :predicate sameletter-fn)

(defun sameletter-fn (w1 p1 w2 p2)
  (equal (char w1 (1- p1)) (char w2 (1- p2))))

(compile 'sameletter-fn)

