;;; define an inequality constraint over words:

(cn:def-lisp-constraint different 
   :interface (x y) 
   :domains (word word)
   :predicate (lambda (x y) (not (equal x y))))

;;; connect all words of the same length pairwise by an inequality constraint:

(cn:connect-pairwise different
   (W1 W6 W20 W35 W36 W42 W51 W110 W115 W135 W144) 
   :medium)

(cn:connect-pairwise different
   (W2 W37 W50 W54 W59 W61 W80 W84 W88 W90 W96 W97 
    W112 W114 W118 W119 W123) 
   :medium)

(cn:connect-pairwise different
